function [ class ] = abpredict(alpha, dsmat, ex)
score = zeros(1, 9);

for t = 1 : 200
    score(dspredict( dsmat(t,1), dsmat(t,1), label0, label1, ex ))
end
double predict = 0.0;
    	for (int t = 0; t < 200; t++) {
    		int p = dsArray[t].predict(ex);
    		if (p == 0) {
    			predict = predict + alpha[t] * -1.0;
    		}
    		else predict = predict + alpha[t] * 1.0;
    	}
    	if(predict >= 0.0)
    		return 1;
    	else return 0;


end

